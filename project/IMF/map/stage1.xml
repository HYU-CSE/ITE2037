<?xml version="1.0"?>
<!--
	@DEFAULT: 무조건 지정해야 합니다.
	@MOST: 지정하는것을 권장합니다.
	@IF: 대부분 옵션입니다.
-->
<!-- @DEFAULT 창 크기는 600x1200 -->
<stage w="5000" h="1600">
	<!-- @MOST stage의 바닥을 정합니다. -->
	<static x="0" y="-400" w="5000" h="15" texture="block_black.png"></static>

	<static x="0" y="-100" w="5000" h="15" texture="block_black.png"></static>
	<!-- @DEFAULT me는 플레이어블 캐릭터를 나타냅니다. -->
	<me x="0" y="0" w="25" h="25" texture="ball.png"></me>

	<static x="0" y="150" w="100" h="15" texture="block_black.png"></static>
	<static x="225" y="100" w="100" h="15" texture="block_black.png"></static>
	<static x="450" y="50" w="100" h="15" texture="block_black.png"></static>

	<static x="-100" y="75" w="15" h="75" texture="block_black.png"></static>
	<static x="100" y="75" w="15" h="75" texture="block_black.png"></static>
</stage>